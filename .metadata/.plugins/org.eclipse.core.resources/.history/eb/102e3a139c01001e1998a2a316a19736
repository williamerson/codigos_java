package entities;

public class Televisor {
	
	public int canal;
	public int volume;
	public int ligado;
	
	public void aumentarVolume() {
		
		if(volume >= 0 && volume <= 16) {
			volume += 1;
		}
		
		public void diminuirrVolume() {
		if(volume >= 0 && volume <= 16) {
			volume -= 1;
		}
		
	}
	
}
